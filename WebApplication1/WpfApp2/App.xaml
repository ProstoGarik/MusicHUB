<Application x:Class="WpfApp2.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfApp2"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="CustomFont">
            <Setter Property="TextElement.FontFamily" Value="Resources/Fonts/#Noto Sans"/>
        </Style>
        <BitmapImage x:Key="exitX" UriSource="Resources/UI/exitX.png"/>
        <BitmapImage x:Key="playTrack" UriSource="Resources/UI/playTrackIcon.png"/>
        <BitmapImage x:Key="volume" UriSource="Resources/UI/volumeIcon.png"/>
        <BitmapImage x:Key="pauseTrack" UriSource="Resources/UI/pauseTrackIcon.png"/>
    </Application.Resources>
</Application>
